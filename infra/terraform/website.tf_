# A non-default, or "aliased" configuration is also defined for a different
# region.
provider "aws" {
  alias  = "moduleregion"
  region = "${var.region}"
}

module "website_with_cname" {
  providers = { aws = "aws.moduleregion" }
  source         = "git::https://github.com/cloudposse/terraform-aws-s3-website.git?ref=master"
  namespace      = "bruno"
  stage          = "dev"
  name           = "app"
  hostname       = "hello.devopssquad.com"
  parent_zone_name = "devopssquad.com"
}
